{
	"plugins": [],
	"recurseDepth": 10,
	"source": {
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"],
        "useLongnameInNav": false,
        "showInheritedInNav": true
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	}
}